{
    "name": "@syngenta-digital/alc",
    "author": "Paul Cruse, Engineering Lead, Syngenta Digital, USCO",
    "main": "src/index.js",
    "types": "typings/index.d.ts",
    "version": "2.0.0-beta.2.3",
    "license": "Apache-2.0",
    "homepage": "https://alc.syngenta-digital.com/node/install/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/syngenta-digital/package-node-alc.git"
    },
    "bugs": {
        "url": "https://github.com/syngenta-digital/package-node-alc/issues"
    },
    "scripts": {
        "ts": "MIN_LOG_LEVEL=OFF mocha test/src/common/event.test.js",
        "test": "MIN_LOG_LEVEL=OFF nyc mocha test/**/**.test.js",
        "report": "MIN_LOG_LEVEL=OFF nyc report --reporter=lcov",
        "lint": "eslint src/**"
    },
    "dependencies": {
        "@apideck/reva": "^0.2.0",
        "ajv": "8.12.0",
        "aws-lambda": "^1.0.7",
        "aws-sdk": "2.1325.0",
        "csv-parse": "^5.3.4",
        "js-yaml": "4.1.0",
        "json-schema-merge-allof": "^0.8.1",
        "json-schema-ref-parser": "9.0.9",
        "xml2js": "0.4.23"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.93",
        "aws-sdk-mock": "^5.7.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "mocha": "^10.1.0",
        "mocha-junit-reporter": "^2.0.0",
        "mocha-multi-reporters": "^1.1.7",
        "mochawesome": "^7.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.0.5",
        "sinon": "^13.0.1"
    },
    "engines": {
        "node": ">=14.17.0"
    },
    "nyc": {
        "temp-directory": "./node_modules/.nyc_output"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "parserOptions": {
            "ecmaVersion": 2020
        },
        "plugins": [
            "prettier"
        ],
        "extends": [
            "plugin:prettier/recommended"
        ],
        "rules": {
            "prettier/prettier": [
                "warn",
                {
                    "trailingComma": "none",
                    "printWidth": 140,
                    "tabWidth": 4,
                    "singleQuote": true,
                    "bracketSpacing": false,
                    "arrowParens": "always"
                }
            ],
            "eqeqeq": "error",
            "prefer-destucturing": 0,
            "no-global-assign": 0,
            "rest-spread-spacing": 1,
            "no-restricted-syntax": 0,
            "consistent-return": 0,
            "import/no-dynamic-require": 0,
            "no-param-reassign": 0,
            "global-require": 0,
            "class-methods-use-this": 0,
            "no-underscore-dangle": 0,
            "camelcase": 0
        }
    }
}
